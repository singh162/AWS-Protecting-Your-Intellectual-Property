{
  "gitSyncId": "6773c710c83f61054013bc2b_da5ffbb1-4129-4c54-b38a-a641eaf4b998",
  "id": "Complaints_sendComplaintsEmail",
  "pluginId": "smtp-plugin",
  "pluginType": "DB",
  "unpublishedAction": {
    "actionConfiguration": {
      "body": "{{complaintEmailTemplates.emailTemplate()}}",
      "encodeParamsToggle": true,
      "formData": {
        "command": "SEND",
        "send": {
          "attachments": "{{ \nAddListInput.listArray\n.map(item => item.FilePicker1)\n.flat() \n.filter(file => file) // Remove any undefined/null values\n}}",
          "bodyType": "text/html",
          "from": "saip@naxap.com",
          "subject": "Compaints for infringment by {{appsmith.store.signUpRightHolderName}} on {{moment.utc().format(\"YYYY-MM-DD hh:mm:ss\")}}",
          "to": "jasmeet.singh@bytescare.com"
        }
      },
      "paginationType": "NONE",
      "timeoutInMillisecond": 10000
    },
    "confirmBeforeExecute": false,
    "datasource": {
      "id": "GMAIL",
      "isAutoGenerated": false,
      "name": "GMAIL",
      "pluginId": "smtp-plugin"
    },
    "dynamicBindingPathList": [
      {
        "key": "formData.send.subject"
      },
      {
        "key": "formData.send.attachments"
      },
      {
        "key": "body"
      }
    ],
    "executeOnLoad": false,
    "name": "sendComplaintsEmail",
    "pageId": "Complaints",
    "userSetOnLoad": false
  }
}